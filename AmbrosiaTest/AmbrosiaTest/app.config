  <appSettings>
    <!-- NOTE!  All of these logs are based off baseAmbrosiaPath variable that is defined in TestInitialize()  -->
    <!--        if source code is at c:\Ambrosia\AmbrosiaTest etc, baseAmbrosiaPath will = c:\Ambrosia\ and that is used as the base for all the paths below  -->
    <!--        These should NOT have a '\' at the beginning as that is part of the base path  -->
    <add key="TestLogOutputDirectory" value="AmbrosiaTest\Log" />
    <!-- NOTE: based on the flag NetFrameworkTestRun in Utilities.cs either net46 or netcoreapp3.1 will be appended to path ” -->
    <add key="PerfTestJobExeWorkingDirectory" value="InternalImmortals\PerformanceTestInterruptible\Client\bin\x64\Release\" />
