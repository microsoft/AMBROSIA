    <add key="AsyncPerfTestServerExeWorkingDirectory" value="InternalImmortals\PerformanceTest\Server\bin\x64\Debug\" />
	<add key="TestRootDirectory" value="AmbrosiaTest\AmbrosiaTest" />
    <add key="TestCMPDirectory" value="AmbrosiaTest\AmbrosiaTest\Cmp" />
    <add key="AmbrosiaLogDirectory" value="AmbrosiaTest\AmbrosiaLogs" />

	  <!-- NOTE: Ambrosia Javascript LB specific values -->
	<add key="AmbrosiaJSTestDirectory" value="AmbrosiaTest\JSTest" />
	<add key="AmbrosiaJSPTIDirectory" value="Clients\AmbrosiaJS\PTI-Node" />

    <add key="ClientSettingsProvider.ServiceUri" value="" />
